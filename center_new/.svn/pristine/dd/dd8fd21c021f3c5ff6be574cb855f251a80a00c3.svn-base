package net.cyweb.model;

import org.springframework.data.mongodb.core.mapping.Document;

import javax.persistence.Transient;

@Document
public class MgYangTrade {
    private String id;
    private Integer tradeId;		// 交易表 交易表的id
    private String tradeNo;		// 订单号
    private Integer memberId;		// 买家uid即member_id
    private Integer currencyId;		// 货币id
    private Integer currencyTradeId;		// currency_trade_id
    private String priceStr;		// 价格

    private Double price;
    private String numStr;		// 数量
    private Double num;
    private String moneyStr;		// money
    private Double money;		// money
    private String feeStr;		// 手续费
    private Double fee;		// 手续费
    private String type;		// buy 或sell
    private Integer addTime;		// 成交时间 （添加表的时间）
    private Integer status;		// status
    private Integer show;		// 默认不显示 1我主动方显示
    private String name;      //姓名
    private String title;    //市场
    private String currencyMark;   //币种简称

    private Integer isShua;//是否系统刷单

    @Transient
    private Integer inputPriceNum;

    @Transient
    private Integer showNum;

    @Transient
    private Integer allMoneyNum;;

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public Integer getTradeId() {
        return tradeId;
    }

    public void setTradeId(Integer tradeId) {
        this.tradeId = tradeId;
    }

    public String getTradeNo() {
        return tradeNo;
    }

    public void setTradeNo(String tradeNo) {
        this.tradeNo = tradeNo;
    }

    public Integer getMemberId() {
        return memberId;
    }

    public void setMemberId(Integer memberId) {
        this.memberId = memberId;
    }

    public Integer getCurrencyId() {
        return currencyId;
    }

    public void setCurrencyId(Integer currencyId) {
        this.currencyId = currencyId;
    }

    public Integer getCurrencyTradeId() {
        return currencyTradeId;
    }

    public void setCurrencyTradeId(Integer currencyTradeId) {
        this.currencyTradeId = currencyTradeId;
    }

    public String getPriceStr() {
        return priceStr;
    }

    public void setPriceStr(String priceStr) {
        this.priceStr = priceStr;
    }

    public Double getPrice() {
        return price;
    }

    public void setPrice(Double price) {
        this.price = price;
    }

    public String getNumStr() {
        return numStr;
    }

    public void setNumStr(String numStr) {
        this.numStr = numStr;
    }

    public Double getNum() {
        return num;
    }

    public void setNum(Double num) {
        this.num = num;
    }

    public String getMoneyStr() {
        return moneyStr;
    }

    public void setMoneyStr(String moneyStr) {
        this.moneyStr = moneyStr;
    }

    public Double getMoney() {
        return money;
    }

    public void setMoney(Double money) {
        this.money = money;
    }

    public String getFeeStr() {
        return feeStr;
    }

    public void setFeeStr(String feeStr) {
        this.feeStr = feeStr;
    }

    public Double getFee() {
        return fee;
    }

    public void setFee(Double fee) {
        this.fee = fee;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public Integer getAddTime() {
        return addTime;
    }

    public void setAddTime(Integer addTime) {
        this.addTime = addTime;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public Integer getShow() {
        return show;
    }

    public void setShow(Integer show) {
        this.show = show;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getCurrencyMark() {
        return currencyMark;
    }

    public void setCurrencyMark(String currencyMark) {
        this.currencyMark = currencyMark;
    }

    public Integer getIsShua() {
        return isShua;
    }

    public void setIsShua(Integer isShua) {
        this.isShua = isShua;
    }

    public Integer getInputPriceNum() {
        return inputPriceNum;
    }

    public void setInputPriceNum(Integer inputPriceNum) {
        this.inputPriceNum = inputPriceNum;
    }

    public Integer getShowNum() {
        return showNum;
    }

    public void setShowNum(Integer showNum) {
        this.showNum = showNum;
    }

    public Integer getAllMoneyNum() {
        return allMoneyNum;
    }

    public void setAllMoneyNum(Integer allMoneyNum) {
        this.allMoneyNum = allMoneyNum;
    }
}
